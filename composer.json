{
  "name": "jcdev/mvc_framework",
  "description": "A basic MVC framework for web/API use - includes basic template system",
  "license": "proprietary",
  "type": "library",

  "keywords": ["MVC","framework","API","web"],
  "authors": [
    {
      "name": "James Carr",
      "email": "james@jamesmcarr.co.uk"
    }
  ],

  "require": {
    "php": "^7.1"

  },
  "require-dev": {
    "phpunit/phpunit": "7.0.0"

  },

  "autoload": {
    "psr-4": {
      "": "app/",
      "abstractClass\\": "framework/abstractClass",
      "constants\\": "framework/constants",
      "enum\\": "framework/enum",
      "controller\\": "framework/controller",
      "factory\\": "framework/factory",
      "frontController\\": "framework/frontController",
      "interfaces\\": "framework/interfaces",
      "model\\": "framework/model/"

    }
  }
}
